{
  "name": "ArcGIS Hub Initiative Admin",
  "version": "1.0",
  "description": "Provides in-browser tools for initiative managers.",
  "permissions": [
    "storage",
    "declarativeContent",
    "identity"
  ],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "ArcGIS Hub Admin"
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "sandbox": {
    "pages": ["sandbox.html"]
  },
  "manifest_version": 2
}